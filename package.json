{
  "name": "@map-colonies/external-to-osm-tag-mapping",
  "version": "1.6.1",
  "description": "map external tags to osm tags",
  "main": "./src/index.ts",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint": "npm run format",
    "prelint:fix": "npm run format:fix",
    "lint:fix": "eslint --fix .",
    "lint:openapi": "openapi lint ./openapi3.yaml",
    "release": "standard-version",
    "test": "npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json  && npm run assets:copy",
    "start": "npm run build && cd dist && node ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles -f ./openapi3.yaml ./dist/ && copyfiles ./package.json dist && copyfiles -f ./schemas.json ./dist/",
    "clean": "rimraf dist"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapcolonies/external-to-osm-tag-mapping.git"
  },
  "author": {
    "name": "MapColonies",
    "email": "mapcolonies@gmail.com",
    "url": "https://github.com/MapColonies"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@godaddy/terminus": "^4.9.0",
    "@map-colonies/error-express-handler": "^2.1.0",
    "@map-colonies/express-access-log-middleware": "0.0.3",
    "@map-colonies/js-logger": "0.0.3",
    "@map-colonies/mc-model-types": "^1.0.1",
    "@map-colonies/openapi-express-viewer": "^2.0.1",
    "@map-colonies/telemetry": "^3.0.0",
    "@opentelemetry/api": "^1.0.1",
    "@opentelemetry/api-metrics": "^0.22.0",
    "@opentelemetry/instrumentation-express": "^0.22.0",
    "@opentelemetry/instrumentation-http": "^0.22.0",
    "@opentelemetry/instrumentation-ioredis": "^0.22.0",
    "ajv": "^8.6.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.2",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.27.11",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.6.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^10.0.0",
    "@map-colonies/eslint-config": "^2.2.1",
    "@map-colonies/standard-version-update-helm-version": "^2.0.1",
    "@map-colonies/standard-version-update-openapi3-version": "0.0.1",
    "@redocly/openapi-cli": "^1.0.0-beta.28",
    "@types/compression": "^1.7.0",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.8",
    "@types/geojson": "^7946.0.8",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.166",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "commitlint": "^9.1.2",
    "copyfiles": "^2.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.1",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-create-mock-instance": "^1.1.0",
    "jest-html-reporters": "^2.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.3.0",
    "type-fest": "^2.3.2",
    "typescript": "^4.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
