{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "metrics": {
      "enabled": true,
      "buckets": [0.1, 0.5, 1, 5, 15, 50, 250, 500]
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "schema": {
    "provider": "file",
    "filePath": "schemas.json"
  },
  "db": {
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "maxRetriesPerRequest": 1
  },
  "application": {
    "hashKey": {
      "enabled": false
    }
  },
  "app": {
    "projectName": "default"
  }
}
